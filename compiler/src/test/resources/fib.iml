program fibonacci
global
	fun fib(i: int32)
	returns r: int32
	requires [ isPos(i) ]
	ensures [ r >= old(i+5) ]
	{
		if(i = 0) {
			r init := 0
		} else {
			if(i = 1){
				r init := 1
			} else {
				r init := fib(i - 1) + fib(i - 2)
			}
		}
	};
	
	fun isPos(i:int32)
	returns b: bool
	{
		b init := i >= 0 
	};
	
	var max: int32
{
	? max init;
	! fib(max)
}