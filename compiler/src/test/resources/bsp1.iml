program intDiv 
global
	proc divide(in copy m:int32, in copy n:int32, out ref q:int32, out ref r:int32)
	requires [n > 0]
	ensures [r >= 0]
	{
	    q init := 0;
	    r init := m;
	
	    while (r >= n) {
	        q := q + 1;
	        r := r - n
	    }
	};
	
	var glob:int32;
	var blub:int32;
	var test:int32
{	
	while(true){
		skip;
		glob init := 1
	};
	
	
	if(true){
		blub init := 1;
		glob init := blub;
		skip
	}else{
		blub init := 2;
		glob init := blub;
		skip
	};
	! glob
}